qiime taxa filter-table \
  --i-table rocky-reef-gut-orders-filtered-table.qza \
  --i-taxonomy taxonomy.qza \
  --p-exclude mitochondria,chloroplast \
  --o-filtered-table table-no-mitochondria-no-chloroplast-rocky-reef-orders.qza

qiime feature-table filter-features \ 
  --i-table table-no-mitochondria-no-chloroplast-rocky-reef-orders.qza \ 
  --p-min-frequency 5 \
  --o-filtered-table rocky_reef_orders_feature-frequency-filtered-table.qza 

qiime feature-table summarize \
  --i-table rocky_reef_orders_feature-frequency-filtered-table.qza \
  --o-visualization rocky_reef_orders_feature-frequency-filtered-table.qzv 

qiime tools export \
   --input-path rocky_reef_orders_feature-frequency-filtered-table.qza \
   --output-path rocky_reef_gut_four_order_picrust
qiime tools export \
   --input-path rep-seqs.qza \
   --output-path rocky_reef_gut_four_order_picrust

conda deactivate
conda activate picrust2

picrust2_pipeline.py \
   -s rocky_reef_gut_four_order_picrust/dna-sequences.fasta \
   -i rocky_reef_gut_four_order_picrust/feature-table.biom \
   -o picrust_out
